@model MvcOtobusBiletleme.Models.Siniflar.CalisanType
@{
    ViewBag.Title = "YeniCalisanType";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br />
<h2>Yeni Çalışan Tipi Sayfası</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.TipAd)
    @Html.TextBoxFor(x => x.TipAd, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Durum)
    @Html.TextBoxFor(x => x.Durum, new { @class = "form-control" })
    <br />
    <button class="btn btn-primary">Kaydet</button>

</form>

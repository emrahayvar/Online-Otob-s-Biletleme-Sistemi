@model MvcOtobusBiletleme.Models.Siniflar.Sehirler
@{
    ViewBag.Title = "SehirEkle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}
<br />
<h2>Şehir Ekleme Sayfası</h2>
<br />
<form class="form-group" method="post">
    @Html.LabelFor(x => x.SehirAd)
    @Html.TextBoxFor(x => x.SehirAd, new { @class = "form-control" })
    <br />
    @Html.LabelFor(x => x.Durum)
    @Html.TextBoxFor(x => x.Durum, new { @class = "form-control" })
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

